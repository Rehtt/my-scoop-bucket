{
    "version": "5.1.8_1",
    "description": "Electronics Design Automation Suite,use tuna mirrors",
    "homepage": "https://kicad.org",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "mirror.tuna.tsinghua.edu.cn/kicad/windows/stable/kicad-5.1.8_1-x86_64.exe#/dl.7z"
        },
        "32bit": {
            "url": "mirror.tuna.tsinghua.edu.cn/kicad/windows/stable/kicad-5.1.8_1-i686.exe#/dl.7z"
        }
    },
    "pre_install": "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Recurse",
    "bin": "bin\\kicad.exe",
    "shortcuts": [
        [
            "bin\\kicad.exe",
            "KiCad"
        ]
    ],
    "env_set": {
        "KICAD_PTEMPLATES": "$dir\\share\\kicad\\template\\",
        "KISYS3DMOD": "$dir\\share\\kicad\\modules\\packages3d\\",
        "KISYSMOD": "$dir\\share\\kicad\\modules\\"
    },
    "checkver": {
        "url": "https://kicad.org/download/windows/",
        "regex": "kicad-([\\d._]+)-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirror.tuna.tsinghua.edu.cn/kicad/windows/stable/kicad-$version-x86_64.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://mirror.tuna.tsinghua.edu.cn/kicad/windows/stable/kicad-$version-i686.exe#/dl.7z"
            }
        }
    }
}